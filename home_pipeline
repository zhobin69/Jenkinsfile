pipeline {
	agent any
	stages {
		stage('Browser Test') {
			parallel {
				stage('Chrome Test') {
					steps {
						echo "Hi there"
						echo "I'm going to test chrome browser"
					}
				}
				stage('FireFox Test') {
					steps {
						echo "Hi again"
						echo "I'm going to test FireFox"
					}
				}
			}
		}
	}
}
